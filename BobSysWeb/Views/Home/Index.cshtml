@model IEnumerable<BobSysWeb.Models.DatabaseDetailModel>

@{
    ViewData["Title"] = "Home Page";
}


<div class="text">
    <table class="table table-dark table-striped table-hover">
        <thead>
            <tr>
                <th scope="col">Job Title</th>
                <th scope="col">Ort</th>              
                <th scope="col"></th>
            </tr>
        </thead>
        <tbody>
           
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.title)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.city)
                </td>              
                <td>                  
                    <a asp-action="Detail" asp-route-id="@item.weMatchid">Details</a>                   
                </td>
            </tr>
        }
        </tbody>
    </table>
